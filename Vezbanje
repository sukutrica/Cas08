import openpyxl as openpyxl
wb=openpyxl.load_workbook("excel1.xlsx")

ws=wb["Stranica 4"]
print(ws.max_column,ws.max_row)

s=ws.max_row

for i in range(2,s+1):
    r=(ws[i])
    for i in r:
        print (i.value)

