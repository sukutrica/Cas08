import psycopg2 as psycopg2
import openpyxl as openpyxl

con=psycopg2.connect(database='KAFE',
user='postgres',
password='itoip',
host='localhost',
port='5432'
)

a=input ("Unesi broj od 1 do 6: ")

def funkcija_one():
    con=psycopg2.connect(database='KAFE',
    user='postgres',
    password='itoip',
    host='localhost',
    port='5432'
    )
    cursor=con.cursor()
    cursor.execute("SELECT NAZIV,CENA FROM KAFE")
    result=cursor.fetchall()
    cursor.close()
    con.close()
    print(result)

def funkcija_TWO():
    con=psycopg2.connect(database='KAFE',
    user='postgres',
    password='itoip',
    host='localhost',
    port='5432'
    )
    cursor=con.cursor()
    cursor.execute("SELECT ID_KAFE,NAZIV FROM KAFE")
    result=cursor.fetchone()
    cursor.close()
    con.close()
    print(result)

def funkcija_tre():
    con=psycopg2.connect(database='KAFE',
    user='postgres',
    password='itoip',
    host='localhost',
    port='5432'
    )
    cursor=con.cursor()
    cursor.execute("SELECT NAZIV,KOFEIN FROM KAFE")
    result=cursor.fetchone()
    cursor.close()
    con.close()
    print(result)

def funkcija_four():
    con=psycopg2.connect(database='KAFE',
    user='postgres',
    password='itoip',
    host='localhost',
    port='5432'
    )
    cursor=con.cursor()
    cursor.execute("SELECT * FROM KAFEE")
    result=cursor.fetchone()
    cursor.close()
    con.close()
    print(result)

def funkcija_five():
    con=psycopg2.connect(database='KAFE',
    user='postgres',
    password='itoip',
    host='localhost',
    port='5432'
    )
    cursor=con.cursor()
    cursor.execute("SELECT NAZIV CENA FROM KAFE WHERE CENA=(SELECT MAX (CENA) FROM KAFE))")
    result=cursor.fetchone()
    cursor.close()
    con.close()
    print(result)

def funkcija_six():
    con=psycopg2.connect(database='KAFE',
    user='postgres',
    password='itoip',
    host='localhost',
    port='5432'
    )
    cursor=con.cursor()
    cursor.execute("SELECT NAZIV KOFEIN FROM KAFE WHERE KOFEIN=(SELECT MAX(KOFEIN) FROM KAFE))")
    result=cursor.fetchone()
    cursor.close()
    con.close()
    print(result)

if a==1:
    print(funkcija_one())
if a==2:
    funkcija_TWO()
if a==3:
    funkcija_tre()
if a==4:
    funkcija_four()
if a==5:
    funkcija_five()
if a==6:
    funkcija_six()
    



